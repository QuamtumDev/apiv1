{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "redirects": [
    {
      "source": "/api/v1/json1",
      "destination": "/index.js"
    },
    {
      "source": "/api/util/encrypt",
      "destination": "index.js"
    },
    {
      "source": "/api/util/decrypt",
      "destination": "index.js"
    }
  ],
  "rewrites": [
    {
      "source": "/url/1",
      "destination": "https://google.com"
    }
  ]
}
